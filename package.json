    "@react-pdf/renderer": "^1.6.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.19",
    "antd": "^4.16.13",
    "axios": "^0.20.0",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "echarts-liquidfill": "^2.0.6",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "moment-business": "^3.0.1",
    "prettier": "^1.19.1",
    "qrcode.react": "^1.0.0",
    "react": "^16.12.0",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.4.5",
    "react-seamless-scroll": "^1.0.3",
    "umi": "^3.2.19",
    "yorkie": "^2.0.0"
